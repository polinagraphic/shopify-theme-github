    "section_DR9bEz": {
      "type": "section",
      "blocks": {
        "text_HXfXrE": {
          "type": "text",
          "name": "t:names.heading",
          "settings": {
            "text": "<h3>Frequently asked questions</h3>",
            "width": "fit-content",
            "max_width": "narrow",
            "alignment": "left",
            "type_preset": "rte",
            "font": "var(--font-body--family)",
            "font_size": "1rem",
            "line_height": "normal",
            "letter_spacing": "normal",
            "case": "none",
            "wrap": "pretty",
            "color": "var(--color-foreground)",
